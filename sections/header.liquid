<header>
    <h1>{{ section.settings.title }}</h1>
    <img src="{{ section.settings.image | img_url }}" alt="">
</header>

{% schema %}
{
    "name": "header",
    "settings": [
        {
            "type": "text",
            "id": "title",
            "label": "test_section"
        },
        {
           "type": "image_picker",
           "id": "image",
           "label": "Header image"
        }
    ]
}
{% endschema %}
